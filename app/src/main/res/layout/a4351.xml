                    <RadioGroup style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/A4363a"
                            style="@style/RadioButton"
                            android:text="@string/rb_A4363_1" />

                        <RadioButton
                            android:id="@+id/A4363_2"
